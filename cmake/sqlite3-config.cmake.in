@PACKAGE_INIT@

# SQLite3 version information
set(SQLITE3_VERSION "@SQLITE3_VERSION@")
set(SQLITE3_VERSION_STRING "@SQLITE3_VERSION@")

# Build configuration options
set(SQLITE3_BUILD_SHARED_LIBS @SQLITE3_BUILD_SHARED_LIBS@)
set(SQLITE3_ENABLE_THREADSAFE @SQLITE3_ENABLE_THREADSAFE@)
set(SQLITE3_ENABLE_DYNAMIC_EXTENSIONS @SQLITE3_ENABLE_DYNAMIC_EXTENSIONS@)
set(SQLITE3_ENABLE_MATH @SQLITE3_ENABLE_MATH@)
set(SQLITE3_ENABLE_FTS3 @SQLITE3_ENABLE_FTS3@)
set(SQLITE3_ENABLE_FTS4 @SQLITE3_ENABLE_FTS4@)
set(SQLITE3_ENABLE_FTS5 @SQLITE3_ENABLE_FTS5@)
set(SQLITE3_ENABLE_RTREE @SQLITE3_ENABLE_RTREE@)
set(SQLITE3_ENABLE_SESSION @SQLITE3_ENABLE_SESSION@)
set(SQLITE3_OMIT_DEPRECATED @SQLITE3_OMIT_DEPRECATED@)

# Import targets
include("${CMAKE_CURRENT_LIST_DIR}/sqlite3-targets.cmake")

# Check required components
check_required_components(sqlite3)
